}
.jetmenu > li.jsright.last > .megamenu,
.jetmenu > li.jsright.last > .megamenu.half-width,
.jetmenu > li.jsright.last > .dropdown{
  right: 0;
}

/* ICONS (FONT AWESOME)
**********************************************************/
.jetmenu > li > a > i{
  line-height: 23px !important;
  margin-right: 6px;
  font-size: 18px;
  float: left;
}

/* COLLAPSIBLE MENU
**********************************************************/
.jetmenu > li.showhide{
  display: none;
  width: 100%;
  height: 50px;
  cursor: pointer;
  color: #999;
  background: #333;
}
