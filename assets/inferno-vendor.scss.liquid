@font-face { font-family: 'slick'; font-weight: normal; font-style: normal; src: url('{{ "slick.eot" | asset_url }}'); src: url('{{ "slick.eot?#iefix" | asset_url }}') format('embedded-opentype'), url('{{ "slick.woff" | asset_url }}') format('woff'), url('{{ "slick.ttf" | asset_url }}') format('truetype'), url('{{ "slick.svg#slick" | asset_url }}') format('svg'); }

